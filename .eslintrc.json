{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": "off",
        "class-methods-use-this": "off",
        "no-await-in-loop": "off"
    }
}
